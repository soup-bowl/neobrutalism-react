{
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"jsx": "react-jsx",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"baseUrl": "./src",
		"paths": {
			//			"@/*": ["*"]
		}
	},
	"include": ["src", ".storybook"],
	"exclude": ["node_modules", "dist"]
}
